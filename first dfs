#include <stdio.h>    

int arr[100][100];
int check[100][100];
int n;

void dfs(int s, int k) {
	check[s][k] = 1;
	for (int h = 0; h < n; h++) {
		if (!check[s][h] && arr[k][h]) dfs(s, h);
	}
}


int main() {
	scanf("%d", &n);
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			scanf("%d", &arr[i][j]);
		}
	}
	for (int i= 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			if (!check[i][j] && arr[i][j]) dfs(i, j);
		}
	}


	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			printf("%d ", check[i][j]);
		}
		printf("\n");
	}
}
